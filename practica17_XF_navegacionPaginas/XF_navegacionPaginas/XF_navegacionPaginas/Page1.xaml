<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF_navegacionPaginas.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Pagina 1" Margin="0,100,0,0" FontSize="30"
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Pagina 1" FontSize="20" Margin="100,30" BackgroundColor="SteelBlue" TextColor="White" Clicked="p1"/>
            <Button Text="Pagina 2" FontSize="20" Margin="100,0" BackgroundColor="SteelBlue" TextColor="White" Clicked="p2"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>